{
   "action": {
      "default_title": "BetterBlox"
   },
   "author": "cupiditys",
   "background": {
      "service_worker": "js/main.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery.js", "js/main.js", "js/pages.js", "js/pages/avatar.js", "js/pages/develop.js", "js/pages/discover.js", "js/pages/games.js", "js/pages/home.js", "js/pages/profile.js", "js/pages/inventory.js", "js/pages/settings.js", "js/pages/transactions.js", "js/pages/universal.js", "js/util.js", "js/pages/vanity.js", "js/pages/groups.js" ],
      "matches": [ "*://*.roblox.com/*" ],
      "run_at": "document_start"
   } ],
   "declarative_net_request": {
      "rule_resources": [ {
         "enabled": true,
         "id": "rules",
         "path": "rules.json"
      } ]
   },
   "default_locale": "en",
   "description": "Adds a variety of useful features to improve Roblox",
   "differential_fingerprint": "1.8b9dbdf99e697669ba61e02fc5e1ea2182d636deb4fa6b6d0818945d342412c8",
   "host_permissions": [ "*://*.roblox.com/*" ],
   "icons": {
      "128": "icons/logo/128/icon.png",
      "256": "icons/logo/256/icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "BetterBlox - Roblox Improved",
   "permissions": [ "storage", "declarativeNetRequest" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.0",
   "web_accessible_resources": [ {
      "matches": [ "*://*.roblox.com/*" ],
      "resources": [ "css/*", "js/*" ]
   } ]
}
